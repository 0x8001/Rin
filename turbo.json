{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "t": {
      "cache": false,
      "dependsOn": ["^test", "^generate"]
    },
    "db:gen": {
        "cache": false,
        "dependsOn": ["^generate"]
    },
    "format:check": {
      "dependsOn": ["^format:check"]
    },
    "format:write": {
      "dependsOn": ["^format:write"]
    }
  }
}
