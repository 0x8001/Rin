    "@elysiajs/jwt": "^1.0.2",
    "arctic": "^1.8.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.10",
    "elysia": "^1.0.20",
    "elysia-oauth2": "^1.2.0",
